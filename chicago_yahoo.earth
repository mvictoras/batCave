<map name="Chicago State Plane" type="projected" version="2">

    <options>
      <profile name="Chicago State Plane (meters)">
			  <srs>+proj=utm +zone=16 +ellps=GRS80 +datum=NAD83 +units=m +no_defs</srs>	
            <xmin>420246.23063342576</xmin>
			<ymin>4607929.595875553</ymin>
            <xmax>458591.13334636507</xmax>
            <ymax>4654384.8787039025</ymax>
        </profile>

	 <cache type="filesystem">
            <path>osg_cache</path>
        </cache>
    </options>

    <!-- This image layer returns global geodetic tiles -->
    <image name="yahoo aerial" driver="yahoo">
        <dataset>satellite</dataset>
    </image>
</map>
